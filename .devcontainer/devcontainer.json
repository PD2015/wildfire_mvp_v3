{
  "name": "WildFire Agent D (Style/System UI)",
  "build": {
    "dockerfile": "Dockerfile",
    "context": ".."
  },
  "forwardPorts": [8084, 5176],
  "portsAttributes": {
    "8084": {
      "label": "Flutter Web App",
      "onAutoForward": "notify"
    },
    "5176": {
      "label": "Dev Server",
      "onAutoForward": "silent"
    }
  },
  "customizations": {
    "vscode": {
      "extensions": [
        "Dart-Code.dart-code",
        "Dart-Code.flutter"
      ],
      "settings": {
        "dart.flutterSdkPath": "/home/vscode/flutter",
        "editor.formatOnSave": true,
        "editor.rulers": [120],
        "dart.lineLength": 120,
        "files.trimTrailingWhitespace": true
      }
    }
  },
  "postCreateCommand": "flutter pub get && flutter doctor",
  "remoteUser": "vscode"
}
